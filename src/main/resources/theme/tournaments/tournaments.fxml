<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<TableView xmlns:fx="http://javafx.com/fxml/1" fx:id="tournamentsTable" maxHeight="1.7976931348623157E308"
           maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" xmlns="http://javafx.com/javafx/8.0.111"
           fx:controller="com.faforever.client.tournament.TournamentsController">
    <columns>
        <TableColumn fx:id="nameColumn" editable="false" minWidth="200.0" prefWidth="500.0" text="%tournament.name"/>
        <TableColumn fx:id="startDateColumn" editable="false" minWidth="100.0" prefWidth="200.0"
                     text="%tournament.startDate"/>
        <TableColumn fx:id="endDateColumn" minWidth="100.0" prefWidth="200.0" text="%tournament.endDate"/>
        <TableColumn fx:id="participantCountColumn" editable="false" minWidth="100.0" prefWidth="100.0"
                     text="%tournament.participantCount"/>
        <TableColumn fx:id="gameModeColumn" editable="false" minWidth="200.0" prefWidth="200.0"
                     text="%tournament.gameMode"/>
        <TableColumn fx:id="detailsColumn" editable="false" minWidth="200.0" prefWidth="200.0"
                     text="%tournament.details"/>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</TableView>
